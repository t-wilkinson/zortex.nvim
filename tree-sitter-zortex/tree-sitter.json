{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "zortex",
      "camelcase": "Zortex",
      "title": "Zortex",
      "scope": "source.zortex",
      "file-types": [
        "zortex"
      ],
      "injection-regex": "^zortex$",
      "class-name": "TreeSitterZortex"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Zortex grammar for tree-sitter",
    "authors": [
      {
        "name": "Trey Wilkinson",
        "email": "winston.trey.wilkinson@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-zortex"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}