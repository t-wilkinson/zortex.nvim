{
  "name": "zortex",
  "version": "0.0.1",
  "description": "all-in-one tool for knowledge management/daily planner/etc.",
  "bin": "./app/local.js",
  "repository": "https://github.com/t-wilkinson/zortex.nvim.git",
  "author": "Trey Wilkinson <winston.trey.wilkinson@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cd app && rm -rf ./.next && next dev",
    "watch": "tsc -w -p ./",
    "build-app": "cd app && rm -rf ./.next && next build && next export",
    "build-lib": "tsc -p ./",
    "build-bin": "cd app && pkg --targets node16-linux-x64,node16-macos-x64,node16-win-x64 --out-path ./bin .",
    "build": "tsc -p ./ && cd app && rm -rf ./.next && next build && next export && yarn && pkg --targets node16-linux-x64,node16-macos-x64,node16-win-x64 --out-path ./bin . && rm -rf ./node_modules ./.next"
  },
  "dependencies": {
    "@chemzqm/neovim": "^5.7.9",
    "chart.js": "^2.7.3",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.3",
    "highlight.js": "^10.4.1",
    "log4js": "^6.4.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^5.2.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-done-right": "^4.0.1",
    "md-it-meta": "^0.0.2",
    "msgpack-lite": "^0.1.26",
    "next": "12",
    "next-routes": "^1.4.2",
    "node-fetch": "2",
    "open": "^8.4.0",
    "plantuml-encoder": "^1.4.0",
    "puppeteer": "^16.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "strftime": "^0.10.1",
    "viz": "^3.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.1",
    "@types/node": "16",
    "@types/socket.io": "^3.0.2",
    "@types/strftime": "^0.9.4",
    "pkg": "^5.6.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.6.4",
    "yuuko-tsconfig": "^1.0.0"
  }
}
