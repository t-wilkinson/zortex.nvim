{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "custom_markdown",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "article_headers"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tags"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_blank_lines"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "body"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "article_headers": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "article_header"
      }
    },
    "article_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@@"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "article_name"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "article_name": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\n]+"
        }
      }
    },
    "tags": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "tag_line"
      }
    },
    "tag_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "tag_name"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "tag_name": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s@\\n]+"
      }
    },
    "_blank_lines": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_blank_line_token"
      }
    },
    "_blank_line_token": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "_newline"
      },
      "named": true,
      "value": "blank_line"
    },
    "body": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_block"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "heading"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "code_block"
        },
        {
          "type": "SYMBOL",
          "name": "latex_block"
        },
        {
          "type": "SYMBOL",
          "name": "paragraph"
        },
        {
          "type": "SYMBOL",
          "name": "_blank_line_token"
        }
      ]
    },
    "heading": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "heading_marker"
          }
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "heading_text"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "heading_marker": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "#+"
      }
    },
    "heading_text": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "link"
          },
          {
            "type": "SYMBOL",
            "name": "formatted_text"
          },
          {
            "type": "SYMBOL",
            "name": "inline_code"
          },
          {
            "type": "SYMBOL",
            "name": "latex_inline"
          },
          {
            "type": "SYMBOL",
            "name": "_escaped_char_generic"
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "PREC",
              "value": -1,
              "content": {
                "type": "PATTERN",
                "value": "[^#*`$\\[\\n\\\\]+"
              }
            }
          }
        ]
      }
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "label_name"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "label_name": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z0-9][A-Za-z0-9 ]*"
      }
    },
    "paragraph": {
      "type": "PREC_DYNAMIC",
      "value": -1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_inline_item"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "_inline_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "link"
        },
        {
          "type": "SYMBOL",
          "name": "formatted_text"
        },
        {
          "type": "SYMBOL",
          "name": "inline_code"
        },
        {
          "type": "SYMBOL",
          "name": "latex_inline"
        },
        {
          "type": "SYMBOL",
          "name": "_escaped_char_generic"
        },
        {
          "type": "SYMBOL",
          "name": "_text_inline"
        }
      ]
    },
    "_text_inline": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -2,
        "content": {
          "type": "PATTERN",
          "value": "([^\\[*`$\\n\\\\]|\\\\.)+"
        }
      }
    },
    "_escaped_char_generic": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\\\."
      }
    },
    "link": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "["
            },
            {
              "type": "FIELD",
              "name": "text",
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "_link_text_content"
                }
              }
            },
            {
              "type": "STRING",
              "value": "]"
            },
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "FIELD",
              "name": "url",
              "content": {
                "type": "REPEAT",
                "content": {
                  "type": "SYMBOL",
                  "name": "_url_content"
                }
              }
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "["
            },
            {
              "type": "FIELD",
              "name": "path",
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "_link_text_content"
                }
              }
            },
            {
              "type": "STRING",
              "value": "]"
            }
          ]
        }
      ]
    },
    "_link_text_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^\\]\\\\]+"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_escaped_char_generic"
        }
      ]
    },
    "_url_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^)\\s\\\\]+"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_escaped_char_generic"
        }
      ]
    },
    "list": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "bullet_list_item"
          },
          {
            "type": "SYMBOL",
            "name": "numbered_list_item"
          }
        ]
      }
    },
    "bullet_list_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "STRING",
                "value": "*"
              },
              {
                "type": "STRING",
                "value": "+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_inline_item"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "numbered_list_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "PATTERN",
            "value": "\\d+\\."
          }
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_inline_item"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "inline_code": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[^`\\n]+"
            }
          }
        },
        {
          "type": "STRING",
          "value": "`"
        }
      ]
    },
    "code_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "```"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "language",
              "content": {
                "type": "SYMBOL",
                "name": "language_name"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_fenced_content"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "```"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "language_name": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z0-9]+"
      }
    },
    "latex_inline": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[^$\\n]+"
            }
          }
        },
        {
          "type": "STRING",
          "value": "$"
        }
      ]
    },
    "latex_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$$"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_fenced_content"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "$$"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_fenced_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[^\\n]+"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "formatted_text": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bold_italic"
        },
        {
          "type": "SYMBOL",
          "name": "bold"
        },
        {
          "type": "SYMBOL",
          "name": "italic"
        }
      ]
    },
    "bold": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "**"
            }
          },
          "named": true,
          "value": "bold_marker_start"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_inline_item_for_formatting"
            }
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "**"
            }
          },
          "named": true,
          "value": "bold_marker_end"
        }
      ]
    },
    "italic": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "*"
            }
          },
          "named": true,
          "value": "italic_marker_start"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_inline_item_for_formatting"
            }
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "*"
            }
          },
          "named": true,
          "value": "italic_marker_end"
        }
      ]
    },
    "bold_italic": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "***"
            }
          },
          "named": true,
          "value": "bold_italic_marker_start"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_inline_item_for_formatting"
            }
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "***"
            }
          },
          "named": true,
          "value": "bold_italic_marker_end"
        }
      ]
    },
    "_inline_item_for_formatting": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "link"
        },
        {
          "type": "SYMBOL",
          "name": "inline_code"
        },
        {
          "type": "SYMBOL",
          "name": "latex_inline"
        },
        {
          "type": "SYMBOL",
          "name": "_escaped_char_generic"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": -1,
            "content": {
              "type": "PATTERN",
              "value": "[^\\[*`$\\n\\\\]+"
            }
          }
        }
      ]
    },
    "_newline": {
      "type": "STRING",
      "value": "\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  ],
  "conflicts": [
    [
      "_text_inline",
      "link"
    ],
    [
      "_text_inline",
      "formatted_text"
    ],
    [
      "_text_inline",
      "inline_code"
    ],
    [
      "_text_inline",
      "latex_inline"
    ]
  ],
  "precedences": [
    [
      {
        "type": "SYMBOL",
        "name": "bold_italic"
      },
      {
        "type": "SYMBOL",
        "name": "bold"
      },
      {
        "type": "SYMBOL",
        "name": "italic"
      }
    ],
    [
      {
        "type": "SYMBOL",
        "name": "code_block"
      },
      {
        "type": "SYMBOL",
        "name": "latex_block"
      },
      {
        "type": "SYMBOL",
        "name": "list"
      },
      {
        "type": "SYMBOL",
        "name": "heading"
      },
      {
        "type": "SYMBOL",
        "name": "label"
      },
      {
        "type": "SYMBOL",
        "name": "paragraph"
      }
    ]
  ],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}