{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "zortex",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "article_header"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "tag_line"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "block"
          }
        }
      ]
    },
    "article_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@@"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "line_content"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "tag_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "line_content"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "blank_line"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "code_block"
        },
        {
          "type": "SYMBOL",
          "name": "latex_block"
        },
        {
          "type": "SYMBOL",
          "name": "heading"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "heading": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "heading_marker"
          }
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "line_content"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "heading_marker": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "#{1,6}"
      }
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "label_name"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "label_name": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9 ][A-Za-z0-9 ]*"
    },
    "list": {
      "type": "PREC_LEFT",
      "value": "list",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "list_item"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "list_item"
            }
          }
        ]
      }
    },
    "list_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "SYMBOL",
                "name": "ordered_marker"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_inline"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "ordered_marker": {
      "type": "PATTERN",
      "value": "\\d+\\."
    },
    "code_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "```"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "language",
              "content": {
                "type": "PATTERN",
                "value": "[A-Za-z0-9_-]+"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "code_line"
                },
                {
                  "type": "STRING",
                  "value": "\n"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "```"
        }
      ]
    },
    "code_line": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "latex_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$$"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "code_line"
                },
                {
                  "type": "STRING",
                  "value": "\n"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "$$"
        }
      ]
    },
    "paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "paragraph_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "paragraph_line"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "paragraph_start": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^@#`$\\-0-9\\n][^\\n]*"
        }
      }
    },
    "paragraph_line": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^@#`$\\-0-9\\n][^\\n]*"
        }
      }
    },
    "_inline": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bolditalic"
        },
        {
          "type": "SYMBOL",
          "name": "bold"
        },
        {
          "type": "SYMBOL",
          "name": "italic"
        },
        {
          "type": "SYMBOL",
          "name": "inline_code"
        },
        {
          "type": "SYMBOL",
          "name": "link"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "bolditalic": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "***"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "STRING",
          "value": "***"
        }
      ]
    },
    "bold": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "**"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "STRING",
          "value": "**"
        }
      ]
    },
    "italic": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "inline_code": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "PATTERN",
          "value": "[^`]+"
        },
        {
          "type": "STRING",
          "value": "`"
        }
      ]
    },
    "link": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "PATTERN",
            "value": "[^\\]]+"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "FIELD",
                  "name": "url",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^)]+"
                  }
                },
                {
                  "type": "STRING",
                  "value": ")"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[^*`\\n\\[\\]]+"
    },
    "line_content": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "blank_line": {
      "type": "STRING",
      "value": "\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t\\f\\r]+"
    }
  ],
  "conflicts": [
    [
      "list",
      "paragraph"
    ]
  ],
  "precedences": [
    [
      {
        "type": "STRING",
        "value": "special_line"
      },
      {
        "type": "STRING",
        "value": "paragraph"
      }
    ],
    [
      {
        "type": "STRING",
        "value": "list"
      },
      {
        "type": "STRING",
        "value": "paragraph"
      }
    ]
  ],
  "externals": [],
  "inline": [],
  "supertypes": []
}
